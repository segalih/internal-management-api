  "scripts": {
    "prebuild": "rimraf dist || true",
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "start": "npm run build && node dist/index.js",
    "serve": "npx nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node -r tsconfig-paths/register -T src/index.ts\"",
    "prestart:dev": "npm run build",
    "start:dev": "npx nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node -r tsconfig-paths/register -T src/index.ts\"",
    "prestart:prod": "npm run build && npx sequelize-cli db:migrate --env production",
    "start:prod": "npm run build && node dist/index.js",
    "dev": "npx ts-node -r tsconfig-paths/register src/index.ts",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit --coverage --verbose",
    "watch": "npm run build && npx concurrently --kill-others \"tsc -w\" \"nodemon -r tsconfig-paths/register dist/index.js\""
  },
